    <!--SQL Query List-->
    <add key="CBNASwapQuery"
      value="SELECT                                      (CASE                                       WHEN LENGTH(reference)=23                                     THEN SUBSTR(a.reference,1,7)                                       ELSE SUBSTR(a.reference,1,8)                                     END) AS ICEdealID                                     , tran_num, deal_tracking_num, a.reference, trade_date, position, price                                      FROM ceiolfprod.ab_tran a                                     WHERE book IN ( 'ICEGateway' )                                     AND trade_date= (                                       select trading_date from ceiolfprod.configuration  )                                     AND trade_flag = 1                                     AND tran_type NOT IN (27, 41)                                     AND tran_status IN (2, 3)                                     AND asset_type = 2                                     and internal_lentity=20328"/>
    <add key="CGMLSwapQuery"
      value="SELECT                                      (CASE                                       WHEN LENGTH(reference)=23                                     THEN SUBSTR(a.reference,1,7)                                       ELSE SUBSTR(a.reference,1,8)                                     END) AS ICEdealID                                     , tran_num, deal_tracking_num, a.reference, trade_date, position, price                                       FROM ceiolfprod.ab_tran  a                                     WHERE book IN ( 'ICEGateway' )                                     AND trade_date= (                                       select trading_date from ceiolfprod.configuration  )                                     AND trade_flag = 1                                     AND tran_type NOT IN (27, 41)                                     AND tran_status IN (2, 3)                                     AND asset_type = 2                                     and internal_lentity in(25274,26163 )                                     "/>
    <add key="CBNAFuturesQuery"
      value="SELECT                                        (CASE                                          WHEN LENGTH(reference)=23                                       THEN SUBSTR(a.reference,1,7)                                          ELSE SUBSTR(a.reference,1,8)                                       END) AS ICEdealID                                       , tran_num, deal_tracking_num, a.reference, trade_date, position, price                                         FROM ceiolfprod.ab_tran a                                       WHERE book IN ( 'IPEGateway' )                                       AND trade_date= (                                         select trading_date from ceiolfprod.configuration  )                                       AND trade_flag = 1                                       AND tran_type NOT IN (27, 41)                                       AND tran_status IN (2, 3)                                       AND asset_type = 2                                       and internal_lentity=20328                                       "/>
    <add key="CGMLFuturesQuery"
      value="SELECT                                        (CASE                                          WHEN LENGTH(reference)=23                                       THEN SUBSTR(a.reference,1,7)                                          ELSE SUBSTR(a.reference,1,8)                                       END) AS ICEdealID                                       , tran_num, deal_tracking_num, a.reference, trade_date, position, price                                         FROM ceiolfprod.ab_tran a                                       WHERE book IN ( 'IPEGateway')                                       AND trade_date= (                                         select trading_date from ceiolfprod.configuration  )                                       AND trade_flag = 1                                       AND tran_type NOT IN (27, 41)                                       AND tran_status IN (2, 3)                                       AND asset_type = 2                                       and internal_lentity in(25274,26163 )                                       "/>
